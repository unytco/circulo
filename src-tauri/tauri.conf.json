{
  "version": "0.26.0",
  "productName": "Unyt",
  "identifier": "co.unyt.unyt.sandbox",
  "build": {
    "beforeBuildCommand": "cd ./unyt && make ci-build-ui",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../unyt/ui/lit-2/dist"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
    "macOS": {
      "entitlements": "entitlements.plist"
    }
  }
}
